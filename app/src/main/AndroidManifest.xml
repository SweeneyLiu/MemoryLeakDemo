<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.lsw.memoryleakdemo">

    <application
        android:name=".MemoryLeakDemoApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".HandlerBadActivity"/>
        <activity android:name=".MatTestActivity">
        </activity>
        <activity android:name=".AsyncTaskLeakActivity">
        </activity>
        <activity android:name=".WebViewActivity"
                  android:process="com.whoislcj.webview">
        </activity>
    </application>

</manifest>